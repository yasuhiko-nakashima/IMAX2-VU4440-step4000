HOST_PIO_W(32'h00000, 32'h000f0000, 256'h0000000000000003000000000000000300000000000000030000000000000003); //REGS
begin : loop18
  forever begin
    @(posedge CLK);
    #(SDELAY);
    HOST_PIO_R(0);
    if (RDDATA[7:0] == 8'h00) begin
      $display("[%t] ----- EMAXSTAT=0 -----",$time);
      disable loop18;
    end
    system_cycle <= system_cycle + 16'h0001;
  end
end
