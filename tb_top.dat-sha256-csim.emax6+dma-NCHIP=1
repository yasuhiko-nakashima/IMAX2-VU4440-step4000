begin : loop10
  forever begin
    @(posedge CLK);
    #(SDELAY);
    HOST_PIO_R(0);
    if (RDDATA[7:0] == 8'h00) begin
      $display("[%t] ----- EMAXSTAT=0 -----",$time);
      disable loop10;
    end
    system_cycle <= system_cycle + 16'h0001;
  end
end
HOST_PIO_W(32'h00020, 32'hff000000, 256'h8008000080080100000000000000000000000000000000000000000000000000); //DMRP +0x20000000
