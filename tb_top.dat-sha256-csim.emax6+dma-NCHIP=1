if (RDDATA != 256'h76828913591c2c92e90ed3807e774c5c0bd04b0155c1d3e9f32e1b161ac36d21) begin
  $display("[%t] ----- ERROR ADDR=%s RDDATA=%h should be 76828913591c2c92_e90ed3807e774c5c_0bd04b0155c1d3e9_f32e1b161ac36d21 -----",$time, "80080100", RDDATA);
  error = error +32'h1;
end
